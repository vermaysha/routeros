{
  "name": "@vermaysha/routeros",
  "version": "1.0.2",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "types": {
        "default": "./dist/index.d.ts"
      },
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup ./src/index.ts --treeshake --shims --format=esm,cjs --clean --env.NODE_ENV production && tsc",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write --unsafe src"
  },
  "keywords": [
    "routeros",
    "routeros-api",
    "node-routeros",
    "bun-routeros",
    "mikrotik-api",
    "mikrotik"
  ],
  "author": {
    "name": "Ashary Vermaysha",
    "email": "vermaysha@gmail.com",
    "url": "https://github.com/vermaysha"
  },
  "license": "MIT",
  "description": "NodeJS / Bun RouterOS API",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "iconv-lite": "^0.6.3"
  }
}
